;Data Stack size         : 1024
;*******************************************************/
;
;#include <mega64a.h>
	#ifndef __SLEEP_DEFINED__
	#define __SLEEP_DEFINED__
	.EQU __se_bit=0x20
	.EQU __sm_mask=0x1C
	.EQU __sm_powerdown=0x10
	.EQU __sm_powersave=0x18
	.EQU __sm_standby=0x14
	.EQU __sm_ext_standby=0x1C
	.EQU __sm_adc_noise_red=0x08
	.SET power_ctrl_reg=mcucr
	#endif
;#include <delay.h>
;#include <i2c.h>
;#include <alcd.h>
;#include <stdio.h>
;#include <DefineSymbols.h>

	.CSEG
_define_char:
	ST   -Y,R26
	ST   -Y,R17
	ST   -Y,R16
;	*pc -> Y+3
;	char_code -> Y+2
;	i -> R17
;	a -> R16
	LDD  R30,Y+2
	LSL  R30
	LSL  R30
	LSL  R30
	ORI  R30,0x40
	MOV  R16,R30
	LDI  R17,LOW(0)
_0x4:
	CPI  R17,8
	BRSH _0x5
	ST   -Y,R16
	INC  R16
	LDD  R30,Y+4
	LDD  R31,Y+4+1
	ADIW R30,1
	STD  Y+4,R30
	STD  Y+4+1,R31
	SBIW R30,1
	LPM  R26,Z
	CALL _lcd_write_byte
	SUBI R17,-1
	RJMP _0x4
_0x5:
	LDD  R17,Y+1
	LDD  R16,Y+0
	ADIW R28,5
	RET
;
;
;signed char recentMenu;
;
;#define volumeMenu    0;
;#define trebleMenu    1;
;#define bassMenu      2;
;#define inputsMenu    3;
;#define preampMenu    4;
;#define loudnessMenu  5;
;#define LRBalanceMenu 6;
;#define FRBalanceMenu 7;
;#define bluetothMenu  8;
;
;
;
;unsigned int timer=0;
;
;unsigned int BlueTimer=0;
;char BlueBuffer[10];
;unsigned char BlueBufferPosition=0;
;unsigned char doesBlueReceiving=0;
;
;eeprom char doesEEpromWritten;
;eeprom int settings[10];
;
;unsigned char volume, treble, bass, input, gain, loudness, brightless;
;signed char LR_balance, FR_balance;
;unsigned int opasity;
;
;
;signed char showtreble, showbass;
;unsigned char doesTurningOff=0;
;
;unsigned int encwas;
;char buffer[64];
;
;                                                                                 //recentMenu
;                                                                                 //   0 - Volume
;#include <ShowScaleSimple.h>                                                     //   1 - Treble
_ShowScaleSimple:
	ST   -Y,R27
	ST   -Y,R26
	ST   -Y,R17
;	input -> Y+1
;	i -> R17
	CALL SUBOPT_0x0
	__POINTW2MN _0x6,0
	CALL SUBOPT_0x1
	LDD  R26,Y+1
	LDD  R27,Y+1+1
	LDI  R30,LOW(5)
	LDI  R31,HIGH(5)
	CALL __DIVW21U
	MOV  R17,R30
_0x8:
	CPI  R17,0
	BREQ _0x9
	LDI  R26,LOW(255)
	CALL _lcd_putchar
	SUBI R17,1
	RJMP _0x8
_0x9:
	LDD  R26,Y+1
	LDD  R27,Y+1+1
	LDI  R30,LOW(5)
	LDI  R31,HIGH(5)
	CALL __MODW21U
	MOV  R17,R30
	MOV  R30,R17
	LDI  R31,0
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0xD
	LDI  R26,LOW(0)
	RJMP _0x13B
_0xD:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0xE
	LDI  R26,LOW(1)
	RJMP _0x13B
_0xE:
	CPI  R30,LOW(0x3)
	LDI  R26,HIGH(0x3)
	CPC  R31,R26
	BRNE _0xF
	LDI  R26,LOW(2)
	RJMP _0x13B
_0xF:
	CPI  R30,LOW(0x4)
	LDI  R26,HIGH(0x4)
	CPC  R31,R26
	BRNE _0xC
	LDI  R26,LOW(3)
_0x13B:
	CALL _lcd_putchar
_0xC:
	LDD  R17,Y+0
	ADIW R28,3
	RET

	.DSEG
_0x6:
	.BYTE 0x11
;#include <ShowScaleBalance.h>                                                    //   2 - Bass

	.CSEG
_ShowScaleBalance:
	ST   -Y,R26
	ST   -Y,R17
	ST   -Y,R16
;	temp -> Y+3
;	temp2 -> Y+2
;	i -> R17
;	k -> R16
	CALL SUBOPT_0x0
	__POINTW2MN _0x11,0
	CALL SUBOPT_0x1
	LDD  R26,Y+2
	CPI  R26,LOW(0x1)
	BRGE _0x12
	LDD  R17,Y+3
	ANDI R17,LOW(15)
	CPI  R17,15
	BREQ _0x13
	LDI  R16,LOW(0)
_0x15:
	CP   R16,R17
	BRSH _0x16
	__POINTW2MN _0x11,17
	CALL _lcd_puts
	SUBI R16,-1
	RJMP _0x15
_0x16:
_0x13:
	CALL SUBOPT_0x2
	LDD  R17,Y+3
	ANDI R17,LOW(15)
	LDI  R16,LOW(15)
_0x18:
	CP   R17,R16
	BRSH _0x19
	__POINTW2MN _0x11,19
	CALL _lcd_puts
	SUBI R16,1
	RJMP _0x18
_0x19:
	RJMP _0x1A
_0x12:
	LDD  R17,Y+3
	ANDI R17,LOW(15)
	LDI  R30,LOW(15)
	SUB  R30,R17
	MOV  R17,R30
	LDI  R16,LOW(0)
_0x1C:
	CPI  R16,7
	BRSH _0x1D
	__POINTW2MN _0x11,21
	CALL _lcd_puts
	SUBI R16,-1
	RJMP _0x1C
_0x1D:
	LDI  R16,LOW(0)
_0x1F:
	CP   R16,R17
	BRSH _0x20
	__POINTW2MN _0x11,23
	CALL _lcd_puts
	SUBI R16,-1
	RJMP _0x1F
_0x20:
	CALL SUBOPT_0x2
	LDI  R16,LOW(0)
_0x22:
	CPI  R16,7
	BRSH _0x23
	__POINTW2MN _0x11,25
	CALL _lcd_puts
	SUBI R16,-1
	RJMP _0x22
_0x23:
_0x1A:
	LDD  R17,Y+1
	LDD  R16,Y+0
	ADIW R28,4
	RET

	.DSEG
_0x11:
	.BYTE 0x1B
;#include <volume_menu.h>                                                         //   3 - Inputs

	.CSEG
_volume_menu:
	ST   -Y,R17
;	temp -> R17
	CALL _lcd_clear
	LDI  R30,LOW(63)
	SUB  R30,R10
	MOV  R17,R30
	CALL SUBOPT_0x3
	__POINTW1FN _0x0,19
	ST   -Y,R31
	ST   -Y,R30
	MOV  R30,R10
	CALL SUBOPT_0x4
	MOV  R26,R10
	CLR  R27
	RCALL _ShowScaleSimple
	LDI  R30,LOW(63)
	AND  R10,R30
	CALL SUBOPT_0x5
	LDI  R26,0
	SBIC 0x1,4
	LDI  R26,1
	LDI  R30,LOW(0)
	CALL __NEB12
	MOV  R0,R30
	LDS  R26,_input
	LDI  R30,LOW(1)
	CALL __EQB12
	AND  R30,R0
	BREQ _0x24
	MOV  R26,R17
	CALL _i2c_write
_0x24:
	CALL _i2c_stop
	RJMP _0x2080007
;#include <treble_menu.h>                                                         //   4 - Preamp
_treble_menu:
	ST   -Y,R17
;	temp2 -> R17
	CALL _lcd_clear
	MOV  R30,R13
	CALL SUBOPT_0x6
	BRNE _0x28
	LDI  R30,LOW(242)
	STS  _showtreble,R30
	LDI  R17,LOW(112)
	RJMP _0x27
_0x28:
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0x29
	LDI  R30,LOW(244)
	STS  _showtreble,R30
	LDI  R17,LOW(113)
	RJMP _0x27
_0x29:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0x2A
	LDI  R30,LOW(246)
	STS  _showtreble,R30
	LDI  R17,LOW(114)
	RJMP _0x27
_0x2A:
	CPI  R30,LOW(0x3)
	LDI  R26,HIGH(0x3)
	CPC  R31,R26
	BRNE _0x2B
	LDI  R30,LOW(248)
	STS  _showtreble,R30
	LDI  R17,LOW(115)
	RJMP _0x27
_0x2B:
	CPI  R30,LOW(0x4)
	LDI  R26,HIGH(0x4)
	CPC  R31,R26
	BRNE _0x2C
	LDI  R30,LOW(250)
	STS  _showtreble,R30
	LDI  R17,LOW(116)
	RJMP _0x27
_0x2C:
	CPI  R30,LOW(0x5)
	LDI  R26,HIGH(0x5)
	CPC  R31,R26
	BRNE _0x2D
	LDI  R30,LOW(252)
	STS  _showtreble,R30
	LDI  R17,LOW(117)
	RJMP _0x27
_0x2D:
	CPI  R30,LOW(0x6)
	LDI  R26,HIGH(0x6)
	CPC  R31,R26
	BRNE _0x2E
	LDI  R30,LOW(254)
	STS  _showtreble,R30
	LDI  R17,LOW(118)
	RJMP _0x27
_0x2E:
	CPI  R30,LOW(0x7)
	LDI  R26,HIGH(0x7)
	CPC  R31,R26
	BRNE _0x2F
	LDI  R30,LOW(0)
	STS  _showtreble,R30
	LDI  R17,LOW(119)
	RJMP _0x27
_0x2F:
	CPI  R30,LOW(0x8)
	LDI  R26,HIGH(0x8)
	CPC  R31,R26
	BRNE _0x30
	LDI  R30,LOW(2)
	STS  _showtreble,R30
	LDI  R17,LOW(126)
	RJMP _0x27
_0x30:
	CPI  R30,LOW(0x9)
	LDI  R26,HIGH(0x9)
	CPC  R31,R26
	BRNE _0x31
	LDI  R30,LOW(4)
	STS  _showtreble,R30
	LDI  R17,LOW(125)
	RJMP _0x27
_0x31:
	CPI  R30,LOW(0xA)
	LDI  R26,HIGH(0xA)
	CPC  R31,R26
	BRNE _0x32
	LDI  R30,LOW(6)
	STS  _showtreble,R30
	LDI  R17,LOW(124)
	RJMP _0x27
_0x32:
	CPI  R30,LOW(0xB)
	LDI  R26,HIGH(0xB)
	CPC  R31,R26
	BRNE _0x33
	LDI  R30,LOW(8)
	STS  _showtreble,R30
	LDI  R17,LOW(123)
	RJMP _0x27
_0x33:
	CPI  R30,LOW(0xC)
	LDI  R26,HIGH(0xC)
	CPC  R31,R26
	BRNE _0x34
	LDI  R30,LOW(10)
	STS  _showtreble,R30
	LDI  R17,LOW(122)
	RJMP _0x27
_0x34:
	CPI  R30,LOW(0xD)
	LDI  R26,HIGH(0xD)
	CPC  R31,R26
	BRNE _0x35
	LDI  R30,LOW(12)
	STS  _showtreble,R30
	LDI  R17,LOW(121)
	RJMP _0x27
_0x35:
	CPI  R30,LOW(0xE)
	LDI  R26,HIGH(0xE)
	CPC  R31,R26
	BRNE _0x27
	LDI  R30,LOW(14)
	STS  _showtreble,R30
	LDI  R17,LOW(120)
_0x27:
	LDS  R26,_showtreble
	CPI  R26,LOW(0x1)
	BRGE _0x37
	CALL SUBOPT_0x3
	__POINTW1FN _0x0,31
	CALL SUBOPT_0x7
_0x37:
	LDS  R26,_showtreble
	CPI  R26,LOW(0x1)
	BRLT _0x38
	CALL SUBOPT_0x3
	__POINTW1FN _0x0,43
	CALL SUBOPT_0x7
_0x38:
	LDI  R26,LOW(_buffer)
	LDI  R27,HIGH(_buffer)
	CALL _lcd_puts
	ST   -Y,R17
	LDS  R26,_showtreble
	RCALL _ShowScaleBalance
	CALL SUBOPT_0x5
	CALL SUBOPT_0x8
	RJMP _0x2080007
;#include <bass_menu.h>                                                           //   5 - Loudness
_bass_menu:
	ST   -Y,R17
;	temp2 -> R17
	CALL _lcd_clear
	MOV  R30,R12
	CALL SUBOPT_0x6
	BRNE _0x3C
	LDI  R30,LOW(242)
	STS  _showbass,R30
	LDI  R17,LOW(96)
	RJMP _0x3B
_0x3C:
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0x3D
	LDI  R30,LOW(244)
	STS  _showbass,R30
	LDI  R17,LOW(97)
	RJMP _0x3B
_0x3D:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0x3E
	LDI  R30,LOW(246)
	STS  _showbass,R30
	LDI  R17,LOW(98)
	RJMP _0x3B
_0x3E:
	CPI  R30,LOW(0x3)
	LDI  R26,HIGH(0x3)
	CPC  R31,R26
	BRNE _0x3F
	LDI  R30,LOW(248)
	STS  _showbass,R30
	LDI  R17,LOW(99)
	RJMP _0x3B
_0x3F:
	CPI  R30,LOW(0x4)
	LDI  R26,HIGH(0x4)
	CPC  R31,R26
	BRNE _0x40
	LDI  R30,LOW(250)
	STS  _showbass,R30
	LDI  R17,LOW(100)
	RJMP _0x3B
_0x40:
	CPI  R30,LOW(0x5)
	LDI  R26,HIGH(0x5)
	CPC  R31,R26
	BRNE _0x41
	LDI  R30,LOW(252)
	STS  _showbass,R30
	LDI  R17,LOW(101)
	RJMP _0x3B
_0x41:
	CPI  R30,LOW(0x6)
	LDI  R26,HIGH(0x6)
	CPC  R31,R26
	BRNE _0x42
	LDI  R30,LOW(254)
	STS  _showbass,R30
	LDI  R17,LOW(102)
	RJMP _0x3B
_0x42:
	CPI  R30,LOW(0x7)
	LDI  R26,HIGH(0x7)
	CPC  R31,R26
	BRNE _0x43
	LDI  R30,LOW(0)
	STS  _showbass,R30
	LDI  R17,LOW(103)
	RJMP _0x3B
_0x43:
	CPI  R30,LOW(0x8)
	LDI  R26,HIGH(0x8)
	CPC  R31,R26
	BRNE _0x44
	LDI  R30,LOW(2)
	STS  _showbass,R30
	LDI  R17,LOW(110)
	RJMP _0x3B
_0x44:
	CPI  R30,LOW(0x9)
	LDI  R26,HIGH(0x9)
	CPC  R31,R26
	BRNE _0x45
	LDI  R30,LOW(4)
	STS  _showbass,R30
	LDI  R17,LOW(109)
	RJMP _0x3B
_0x45:
	CPI  R30,LOW(0xA)
	LDI  R26,HIGH(0xA)
	CPC  R31,R26
	BRNE _0x46
	LDI  R30,LOW(6)
	STS  _showbass,R30
	LDI  R17,LOW(108)
	RJMP _0x3B
_0x46:
	CPI  R30,LOW(0xB)
	LDI  R26,HIGH(0xB)
	CPC  R31,R26
	BRNE _0x47
	LDI  R30,LOW(8)
	STS  _showbass,R30
	LDI  R17,LOW(107)
	RJMP _0x3B
_0x47:
	CPI  R30,LOW(0xC)
	LDI  R26,HIGH(0xC)
	CPC  R31,R26
	BRNE _0x48
	LDI  R30,LOW(10)
	STS  _showbass,R30
	LDI  R17,LOW(106)
	RJMP _0x3B
_0x48:
	CPI  R30,LOW(0xD)
	LDI  R26,HIGH(0xD)
	CPC  R31,R26
	BRNE _0x49
	LDI  R30,LOW(12)
	STS  _showbass,R30
	LDI  R17,LOW(105)
	RJMP _0x3B
_0x49:
	CPI  R30,LOW(0xE)
	LDI  R26,HIGH(0xE)
	CPC  R31,R26
	BRNE _0x3B
	LDI  R30,LOW(14)
	STS  _showbass,R30
	LDI  R17,LOW(104)
_0x3B:
	LDS  R26,_showbass
	CPI  R26,LOW(0x1)
	BRGE _0x4B
	CALL SUBOPT_0x3
	__POINTW1FN _0x0,56
	CALL SUBOPT_0x9
_0x4B:
	LDS  R26,_showbass
	CPI  R26,LOW(0x1)
	BRLT _0x4C
	CALL SUBOPT_0x3
	__POINTW1FN _0x0,66
	CALL SUBOPT_0x9
_0x4C:
	CALL SUBOPT_0xA
	ST   -Y,R17
	LDS  R26,_showbass
	RCALL _ShowScaleBalance
	CALL SUBOPT_0x5
	CALL SUBOPT_0x8
	RJMP _0x2080007
;#include <inputs_menu.h>                                                         //   6 - Balance Left - Right
_inputs_menu:
	ST   -Y,R17
;	send -> R17
	CALL _lcd_clear
	CALL SUBOPT_0x0
	LDS  R30,_loudness
	ANDI R30,LOW(0x4)
	STS  _loudness,R30
	LDS  R30,_gain
	ANDI R30,LOW(0x18)
	STS  _gain,R30
	LDS  R30,_input
	ANDI R30,LOW(0x3)
	STS  _input,R30
	LDI  R30,LOW(0)
	CALL SUBOPT_0xB
	__POINTW2MN _0x4D,0
	CALL _lcd_puts
	LDS  R30,_input
	CALL SUBOPT_0x6
	BRNE _0x51
	LDI  R30,LOW(10)
	RJMP _0x13C
_0x51:
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0x52
	LDI  R30,LOW(12)
	RJMP _0x13C
_0x52:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0x50
	LDI  R30,LOW(14)
_0x13C:
	ST   -Y,R30
	LDI  R26,LOW(1)
	CALL _lcd_gotoxy
_0x50:
	LDI  R26,LOW(217)
	CALL _lcd_putchar
	CALL SUBOPT_0xC
	CALL SUBOPT_0x8
	RJMP _0x2080007

	.DSEG
_0x4D:
	.BYTE 0x11
;#include <preamp_menu.h>

	.CSEG
_preamp_menu:
	ST   -Y,R17
;	send -> R17
	CALL _lcd_clear
	LDS  R30,_input
	ANDI R30,LOW(0x3)
	STS  _input,R30
	CALL SUBOPT_0x6
	BRNE _0x57
	CALL SUBOPT_0xD
	CPI  R30,LOW(0x18)
	LDI  R26,HIGH(0x18)
	CPC  R31,R26
	BRNE _0x5B
	__POINTW2MN _0x5C,0
	CALL SUBOPT_0x1
	__POINTW2MN _0x5C,17
	RJMP _0x13D
_0x5B:
	CPI  R30,LOW(0x10)
	LDI  R26,HIGH(0x10)
	CPC  R31,R26
	BRNE _0x5D
	__POINTW2MN _0x5C,34
	CALL SUBOPT_0x1
	__POINTW2MN _0x5C,51
	RJMP _0x13D
_0x5D:
	CPI  R30,LOW(0x8)
	LDI  R26,HIGH(0x8)
	CPC  R31,R26
	BRNE _0x5E
	__POINTW2MN _0x5C,68
	CALL SUBOPT_0x1
	__POINTW2MN _0x5C,85
	RJMP _0x13D
_0x5E:
	SBIW R30,0
	BRNE _0x5A
	__POINTW2MN _0x5C,102
	CALL SUBOPT_0x1
	__POINTW2MN _0x5C,119
_0x13D:
	CALL _lcd_puts
_0x5A:
	RJMP _0x56
_0x57:
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BREQ _0x61
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0x56
_0x61:
	CALL SUBOPT_0xD
_0x66:
	CPI  R30,LOW(0x18)
	LDI  R26,HIGH(0x18)
	CPC  R31,R26
	BRNE _0x67
	__POINTW2MN _0x5C,136
	CALL SUBOPT_0x1
	__POINTW2MN _0x5C,153
	CALL _lcd_puts
	RJMP _0x65
_0x67:
	CPI  R30,LOW(0x10)
	LDI  R26,HIGH(0x10)
	CPC  R31,R26
	BRNE _0x68
	__POINTW2MN _0x5C,164
	CALL _lcd_puts
	LDI  R30,LOW(7)
	ST   -Y,R30
	LDI  R26,LOW(1)
	CALL _lcd_gotoxy
	__POINTW2MN _0x5C,181
	CALL _lcd_puts
	RJMP _0x65
_0x68:
	CPI  R30,LOW(0x8)
	LDI  R26,HIGH(0x8)
	CPC  R31,R26
	BRNE _0x69
	LDI  R30,LOW(16)
	STS  _gain,R30
	RJMP _0x66
_0x69:
	SBIW R30,0
	BRNE _0x65
	LDI  R30,LOW(16)
	STS  _gain,R30
	RJMP _0x66
_0x65:
_0x56:
	CALL SUBOPT_0xC
	CALL SUBOPT_0x8
	RJMP _0x2080007

	.DSEG
_0x5C:
	.BYTE 0xB7
;#include <loudness_menu.h>

	.CSEG
_loudness_menu:
	ST   -Y,R17
;	send -> R17
	CALL _lcd_clear
	__POINTW2MN _0x6C,0
	CALL SUBOPT_0x1
	LDS  R30,_loudness
	CPI  R30,0
	BRNE _0x6D
	__POINTW2MN _0x6C,10
	RJMP _0x13E
_0x6D:
	__POINTW2MN _0x6C,26
_0x13E:
	CALL _lcd_puts
	CALL SUBOPT_0xC
	CALL SUBOPT_0x8
	RJMP _0x2080007

	.DSEG
_0x6C:
	.BYTE 0x2B
;#include <RL_balance_menu.h>

	.CSEG
_RL_balance_menu:
	CALL SUBOPT_0xE
;	balance1 -> R17
;	balance2 -> R16
;	LF -> R19
;	RF -> R18
;	LR -> R21
;	RR -> R20
;	x -> Y+6
	BRGE _0x6F
	MOV  R18,R17
	LDI  R19,LOW(31)
_0x6F:
	LDS  R17,_LR_balance
	CPI  R17,31
	BRNE _0x70
	LDI  R30,LOW(31)
	MOV  R19,R30
	MOV  R18,R30
_0x70:
	LDS  R17,_LR_balance
	CPI  R17,32
	BRLT _0x71
	LDI  R30,LOW(62)
	SUB  R30,R17
	MOV  R17,R30
	MOV  R19,R17
	LDI  R18,LOW(31)
_0x71:
	MOV  R21,R19
	MOV  R20,R18
	CALL SUBOPT_0xF
	__POINTW1FN _0x0,286
	ST   -Y,R31
	ST   -Y,R30
	MOV  R30,R19
	CALL SUBOPT_0x10
	LDI  R30,LOW(7)
	CALL SUBOPT_0xB
	CALL SUBOPT_0x11
	CALL SUBOPT_0x12
	MOV  R30,R18
	CALL SUBOPT_0x10
	LDI  R30,LOW(11)
	CALL SUBOPT_0xB
	__POINTW2MN _0x72,0
	CALL SUBOPT_0x1
	CPI  R19,0
	BREQ _0x73
	LDS  R17,_LR_balance
_0x75:
	CPI  R17,1
	BRLT _0x76
	__POINTW2MN _0x72,6
	CALL SUBOPT_0x13
	SUBI R17,LOW(5)
	RJMP _0x75
_0x76:
	CALL SUBOPT_0x2
_0x77:
	LDD  R30,Y+6
	CPI  R30,0
	BREQ _0x79
	__POINTW2MN _0x72,8
	CALL SUBOPT_0x13
	RJMP _0x77
_0x79:
	RJMP _0x7A
_0x73:
	__POINTW2MN _0x72,10
	CALL _lcd_puts
	CALL SUBOPT_0x2
_0x7A:
	CPI  R16,31
	BRGE _0x7B
	LDI  R30,LOW(31)
	SUB  R30,R16
	MOV  R16,R30
	SUB  R20,R16
	SUB  R21,R16
_0x7B:
	LDS  R16,_FR_balance
	CPI  R16,32
	BRLT _0x7C
	SUBI R16,LOW(31)
	SUB  R18,R16
	SUB  R19,R16
_0x7C:
	CALL SUBOPT_0x5
	CPI  R19,1
	BRLT _0x7D
	LDI  R30,LOW(31)
	SUB  R30,R19
	MOV  R19,R30
	RJMP _0x7E
_0x7D:
	LDI  R19,LOW(31)
_0x7E:
	CPI  R18,1
	BRLT _0x7F
	LDI  R30,LOW(31)
	SUB  R30,R18
	MOV  R18,R30
	RJMP _0x80
_0x7F:
	LDI  R18,LOW(31)
_0x80:
	CPI  R21,1
	BRLT _0x81
	LDI  R30,LOW(31)
	SUB  R30,R21
	MOV  R21,R30
	RJMP _0x82
_0x81:
	LDI  R21,LOW(31)
_0x82:
	CPI  R20,1
	BRLT _0x83
	LDI  R30,LOW(31)
	SUB  R30,R20
	MOV  R20,R30
	RJMP _0x84
_0x83:
	LDI  R20,LOW(31)
_0x84:
	RJMP _0x2080008

	.DSEG
_0x72:
	.BYTE 0x19
;#include <FR_balance_menu.h>

	.CSEG
_FS_balance_menu:
	CALL SUBOPT_0xE
;	balance1 -> R17
;	balance2 -> R16
;	LF -> R19
;	RF -> R18
;	LR -> R21
;	RR -> R20
;	x -> Y+6
	BRGE _0x85
	MOV  R18,R17
	LDI  R19,LOW(31)
_0x85:
	LDS  R17,_LR_balance
	CPI  R17,31
	BRNE _0x86
	LDI  R30,LOW(31)
	MOV  R19,R30
	MOV  R18,R30
_0x86:
	LDS  R17,_LR_balance
	CPI  R17,32
	BRLT _0x87
	LDI  R30,LOW(62)
	SUB  R30,R17
	MOV  R17,R30
	MOV  R19,R17
	LDI  R18,LOW(31)
_0x87:
	MOV  R21,R19
	MOV  R20,R18
	CPI  R16,31
	BRGE _0x88
	LDI  R30,LOW(31)
	SUB  R30,R16
	MOV  R16,R30
	SUB  R20,R16
	SUB  R21,R16
_0x88:
	LDS  R16,_FR_balance
	CPI  R16,32
	BRLT _0x89
	SUBI R16,LOW(31)
	SUB  R18,R16
	SUB  R19,R16
_0x89:
	LDS  R16,_FR_balance
	CPI  R16,31
	BRGE _0x8A
	CALL SUBOPT_0xF
	CALL SUBOPT_0x14
	LDI  R30,LOW(8)
	CALL SUBOPT_0xB
	CALL SUBOPT_0x11
	CALL SUBOPT_0x12
	MOV  R30,R16
	CALL SUBOPT_0x10
	LDI  R30,LOW(12)
	CALL SUBOPT_0xB
	__POINTW2MN _0x8B,0
	CALL _lcd_puts
_0x8A:
	CPI  R16,31
	BRNE _0x8C
	CALL SUBOPT_0xF
	CALL SUBOPT_0x14
	LDI  R30,LOW(8)
	CALL SUBOPT_0xB
	CALL SUBOPT_0x11
	CALL SUBOPT_0x12
	CALL SUBOPT_0x15
	LDI  R30,LOW(12)
	CALL SUBOPT_0xB
	__POINTW2MN _0x8B,5
	CALL _lcd_puts
_0x8C:
	CPI  R16,32
	BRLT _0x8D
	LDI  R30,LOW(62)
	SUB  R30,R16
	MOV  R16,R30
	CALL SUBOPT_0xF
	__POINTW1FN _0x0,315
	ST   -Y,R31
	ST   -Y,R30
	MOV  R30,R16
	CALL SUBOPT_0x10
	LDI  R30,LOW(8)
	CALL SUBOPT_0xB
	CALL SUBOPT_0x11
	CALL SUBOPT_0x12
	CALL SUBOPT_0x15
	LDI  R30,LOW(12)
	CALL SUBOPT_0xB
	__POINTW2MN _0x8B,10
	CALL _lcd_puts
_0x8D:
	LDS  R16,_FR_balance
	CPI  R16,62
	BREQ _0x8E
	LDS  R16,_FR_balance
_0x90:
	CPI  R16,1
	BRLT _0x91
	__POINTW2MN _0x8B,15
	CALL SUBOPT_0x13
	SUBI R16,LOW(5)
	RJMP _0x90
_0x91:
	CALL SUBOPT_0x2
_0x92:
	LDD  R30,Y+6
	CPI  R30,0
	BREQ _0x94
	__POINTW2MN _0x8B,17
	CALL SUBOPT_0x13
	RJMP _0x92
_0x94:
	RJMP _0x95
_0x8E:
	__POINTW2MN _0x8B,19
	CALL _lcd_puts
	CALL SUBOPT_0x2
_0x95:
	CALL SUBOPT_0x5
	CPI  R19,1
	BRLT _0x96
	LDI  R30,LOW(31)
	SUB  R30,R19
	MOV  R19,R30
	RJMP _0x97
_0x96:
	LDI  R19,LOW(31)
_0x97:
	CPI  R18,1
	BRLT _0x98
	LDI  R30,LOW(31)
	SUB  R30,R18
	MOV  R18,R30
	RJMP _0x99
_0x98:
	LDI  R18,LOW(31)
_0x99:
	CPI  R21,1
	BRLT _0x9A
	LDI  R30,LOW(31)
	SUB  R30,R21
	MOV  R21,R30
	RJMP _0x9B
_0x9A:
	LDI  R21,LOW(31)
_0x9B:
	CPI  R20,1
	BRLT _0x9C
	LDI  R30,LOW(31)
	SUB  R30,R20
	MOV  R20,R30
	RJMP _0x9D
_0x9C:
	LDI  R20,LOW(31)
_0x9D:
_0x2080008:
	ANDI R19,LOW(31)
	ANDI R18,LOW(31)
	ANDI R21,LOW(31)
	ANDI R20,LOW(31)
	ORI  R19,LOW(128)
	ORI  R18,LOW(160)
	ORI  R21,LOW(192)
	ORI  R20,LOW(224)
	MOV  R26,R19
	CALL _i2c_write
	MOV  R26,R18
	CALL _i2c_write
	MOV  R26,R21
	CALL _i2c_write
	MOV  R26,R20
	CALL _i2c_write
	CALL _i2c_stop
	CALL __LOADLOCR6
	ADIW R28,7
	RET

	.DSEG
_0x8B:
	.BYTE 0x22
;#include <bluetooth_menu.h>

	.CSEG
_bluetooth_menu:
	CALL _lcd_clear
	__POINTW2MN _0x9E,0
	CALL SUBOPT_0x1
	SBIS 0x19,1
	RJMP _0x9F
	__POINTW2MN _0x9E,13
	RJMP _0x13F
_0x9F:
	__POINTW2MN _0x9E,25
_0x13F:
	CALL _lcd_puts
	RET

	.DSEG
_0x9E:
	.BYTE 0x25
;#include <brightless_menu.h>

	.CSEG
_brightless_menu:
	ST   -Y,R17
;	temp -> R17
	LDS  R30,_brightless
	OUT  0x31,R30
	MOV  R17,R30
	MOV  R26,R17
	LDI  R27,0
	LDI  R30,LOW(5)
	LDI  R31,HIGH(5)
	CALL __DIVW21
	MOV  R17,R30
	CALL SUBOPT_0xF
	__POINTW1FN _0x0,366
	ST   -Y,R31
	ST   -Y,R30
	MOV  R30,R17
	CALL SUBOPT_0x4
	MOV  R26,R17
	CLR  R27
	RCALL _ShowScaleSimple
_0x2080007:
	LD   R17,Y+
	RET
;#include <opasity_menu.h>
_opasity_menu:
	ST   -Y,R17
	ST   -Y,R16
;	temp -> R16,R17
	__GETWRMN 16,17,0,_opasity
	CALL SUBOPT_0x16
	OUT  0x2A+1,R31
	OUT  0x2A,R30
	MOVW R26,R16
	LDI  R30,LOW(20)
	LDI  R31,HIGH(20)
	CALL __DIVW21
	MOVW R16,R30
	CALL SUBOPT_0xF
	__POINTW1FN _0x0,382
	ST   -Y,R31
	ST   -Y,R30
	MOVW R30,R16
	CALL __CWD1
	CALL __PUTPARD1
	LDI  R24,4
	CALL _sprintf
	ADIW R28,8
	CALL SUBOPT_0xA
	MOVW R26,R16
	RCALL _ShowScaleSimple
	RJMP _0x2080005
;
;
;interrupt [USART1_RXC] void usart1_rx_isr(void) {char k,data;
; 0000 0055 interrupt [31] void usart1_rx_isr(void) {char k,data;
_usart1_rx_isr:
	CALL SUBOPT_0x17
; 0000 0056 
; 0000 0057 if (BlueTimer<500) {
	ST   -Y,R17
	ST   -Y,R16
;	k -> R17
;	data -> R16
	LDI  R30,LOW(500)
	LDI  R31,HIGH(500)
	CP   R8,R30
	CPC  R9,R31
	BRSH _0xA1
; 0000 0058 data=UDR1;
	LDS  R16,156
; 0000 0059 if ((data!=0x0D)&(data!=0x0A)) {
	MOV  R26,R16
	LDI  R30,LOW(13)
	CALL __NEB12
	MOV  R0,R30
	LDI  R30,LOW(10)
	CALL __NEB12
	AND  R30,R0
	BREQ _0xA2
; 0000 005A BlueTimer=0;
	CLR  R8
	CLR  R9
; 0000 005B doesBlueReceiving=1;
	LDI  R30,LOW(1)
	MOV  R11,R30
; 0000 005C BlueBuffer[BlueBufferPosition]=data;
	MOV  R30,R4
	LDI  R31,0
	SUBI R30,LOW(-_BlueBuffer)
	SBCI R31,HIGH(-_BlueBuffer)
	ST   Z,R16
; 0000 005D BlueBufferPosition++;
	INC  R4
; 0000 005E }
; 0000 005F 
; 0000 0060 } else {
_0xA2:
	RJMP _0xA3
_0xA1:
; 0000 0061 doesBlueReceiving=0;
	CLR  R11
; 0000 0062 BlueBufferPosition=0;
	CLR  R4
; 0000 0063 BlueTimer=0;
	CLR  R8
	CLR  R9
; 0000 0064 recentMenu=15;
	LDI  R30,LOW(15)
	MOV  R5,R30
; 0000 0065 lcd_clear();
	CALL _lcd_clear
; 0000 0066 lcd_puts("Bluetooth said: ");
	__POINTW2MN _0xA4,0
	CALL _lcd_puts
; 0000 0067 lcd_puts(BlueBuffer);
	LDI  R26,LOW(_BlueBuffer)
	LDI  R27,HIGH(_BlueBuffer)
	CALL _lcd_puts
; 0000 0068 for (k=0; k<10; k++) {BlueBuffer[k]=0;}
	LDI  R17,LOW(0)
_0xA6:
	CPI  R17,10
	BRSH _0xA7
	MOV  R30,R17
	LDI  R31,0
	SUBI R30,LOW(-_BlueBuffer)
	SBCI R31,HIGH(-_BlueBuffer)
	LDI  R26,LOW(0)
	STD  Z+0,R26
	SUBI R17,-1
	RJMP _0xA6
_0xA7:
; 0000 0069 }
_0xA3:
; 0000 006A }
	RJMP _0x148

	.DSEG
_0xA4:
	.BYTE 0x11
;
;interrupt [USART1_TXC] void usart1_tx_isr(void) {
; 0000 006C interrupt [33] void usart1_tx_isr(void) {

	.CSEG
_usart1_tx_isr:
; 0000 006D 
; 0000 006E }
	RETI
