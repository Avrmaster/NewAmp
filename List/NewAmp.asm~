;void SendToBluetoothByte (char data) {
; 0000 0070 void SendToBluetoothByte (char data) {
_SendToBluetoothByte:
; 0000 0071 
; 0000 0072 while ( !( UCSR1A & (1<<UDRE1)) ) {}
	ST   -Y,R26
;	data -> Y+0
_0xA8:
	LDS  R30,155
	ANDI R30,LOW(0x20)
	BREQ _0xA8
; 0000 0073 UDR1=data;
	LD   R30,Y
	STS  156,R30
; 0000 0074 }
	JMP  _0x2080002
;
;void SendToBluetooth (char *string){
; 0000 0076 void SendToBluetooth (char *string){
_SendToBluetooth:
; 0000 0077 SendToBluetoothByte('A');
	ST   -Y,R27
	ST   -Y,R26
;	*string -> Y+0
	LDI  R26,LOW(65)
	RCALL _SendToBluetoothByte
; 0000 0078 SendToBluetoothByte('T');
	LDI  R26,LOW(84)
	RCALL _SendToBluetoothByte
; 0000 0079 SendToBluetoothByte('#');
	LDI  R26,LOW(35)
	RCALL _SendToBluetoothByte
; 0000 007A SendToBluetoothByte(*string);
	LD   R26,Y
	LDD  R27,Y+1
	LD   R26,X
	RCALL _SendToBluetoothByte
; 0000 007B *string++;
	LD   R26,Y
	LDD  R27,Y+1
	LD   R30,X+
	ST   Y,R26
	STD  Y+1,R27
; 0000 007C SendToBluetoothByte(*string);
	LD   R26,X
	RCALL _SendToBluetoothByte
; 0000 007D SendToBluetoothByte(0x0a);
	LDI  R26,LOW(10)
	RCALL _SendToBluetoothByte
; 0000 007E SendToBluetoothByte(0x0d);
	LDI  R26,LOW(13)
	RCALL _SendToBluetoothByte
; 0000 007F 
; 0000 0080 }
	JMP  _0x2080003
;
;void LED (unsigned char color) {
; 0000 0082 void LED (unsigned char color) {
_LED:
; 0000 0083 switch (color) {
	ST   -Y,R26
;	color -> Y+0
	LD   R30,Y
	CALL SUBOPT_0x6
; 0000 0084 case 0: PORTF=0b00000000; break;
	BRNE _0xAE
	LDI  R30,LOW(0)
	RJMP _0x140
; 0000 0085 case 1: PORTF=0b00000100; break;
_0xAE:
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0xAF
	LDI  R30,LOW(4)
	RJMP _0x140
; 0000 0086 case 2: PORTF=0b00010000; break;
_0xAF:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0xB0
	LDI  R30,LOW(16)
	RJMP _0x140
; 0000 0087 case 3: PORTF=0b01000000; break;
_0xB0:
	CPI  R30,LOW(0x3)
	LDI  R26,HIGH(0x3)
	CPC  R31,R26
	BRNE _0xB1
	LDI  R30,LOW(64)
	RJMP _0x140
; 0000 0088 case 4: PORTF=0b00010100; break;
_0xB1:
	CPI  R30,LOW(0x4)
	LDI  R26,HIGH(0x4)
	CPC  R31,R26
	BRNE _0xB2
	LDI  R30,LOW(20)
	RJMP _0x140
; 0000 0089 case 5: PORTF=0b01000100; break;
_0xB2:
	CPI  R30,LOW(0x5)
	LDI  R26,HIGH(0x5)
	CPC  R31,R26
	BRNE _0xB3
	LDI  R30,LOW(68)
	RJMP _0x140
; 0000 008A case 6: PORTF=0b01010000; break;
_0xB3:
	CPI  R30,LOW(0x6)
	LDI  R26,HIGH(0x6)
	CPC  R31,R26
	BRNE _0xB4
	LDI  R30,LOW(80)
	RJMP _0x140
; 0000 008B case 7: PORTF=0b01010100; break;
_0xB4:
	CPI  R30,LOW(0x7)
	LDI  R26,HIGH(0x7)
	CPC  R31,R26
	BRNE _0xAD
	LDI  R30,LOW(84)
_0x140:
	STS  98,R30
; 0000 008C }
_0xAD:
; 0000 008D }
	JMP  _0x2080002
;
;void BluetoothOFF (void) {
; 0000 008F void BluetoothOFF (void) {
_BluetoothOFF:
; 0000 0090 PORTA.1=0;
	CBI  0x1B,1
; 0000 0091 UCSR1A=0x00;
	LDI  R30,LOW(0)
	STS  155,R30
; 0000 0092 UCSR1B=0x00;
	STS  154,R30
; 0000 0093 UCSR1C=0x00;
	CALL SUBOPT_0x18
; 0000 0094 UBRR1H=0x00;
; 0000 0095 UBRR1L=0x00;
	LDI  R30,LOW(0)
	RJMP _0x2080006
; 0000 0096 }
;
;void BluetoothON (void) {
; 0000 0098 void BluetoothON (void) {
_BluetoothON:
; 0000 0099 PORTA.1=1;
	SBI  0x1B,1
; 0000 009A UCSR1A=0x00;
	CALL SUBOPT_0x19
; 0000 009B UCSR1B=0xD8;
; 0000 009C UCSR1C=0x06;
; 0000 009D UBRR1H=0x00;
; 0000 009E UBRR1L=0x08;
	LDI  R30,LOW(8)
_0x2080006:
	STS  153,R30
; 0000 009F }
	RET
;
