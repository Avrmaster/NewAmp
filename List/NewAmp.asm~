;//Update SCREEN information
;void UpdateMenu (void) {
; 0000 00A3 void UpdateMenu (void) {
_UpdateMenu:
; 0000 00A4 	switch (recentMenu) {
	MOV  R30,R5
	LDI  R31,0
	SBRC R30,7
	SER  R31
; 0000 00A5 		case 0: volume_menu();
	SBIW R30,0
	BRNE _0xBD
	RCALL _volume_menu
; 0000 00A6 		break;
	RJMP _0xBC
; 0000 00A7 		case 1: treble_menu();
_0xBD:
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0xBE
	RCALL _treble_menu
; 0000 00A8 		break;
	RJMP _0xBC
; 0000 00A9 		case 2: bass_menu();
_0xBE:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0xBF
	RCALL _bass_menu
; 0000 00AA 		break;
	RJMP _0xBC
; 0000 00AB 		case 3: inputs_menu();
_0xBF:
	CPI  R30,LOW(0x3)
	LDI  R26,HIGH(0x3)
	CPC  R31,R26
	BRNE _0xC0
	RCALL _inputs_menu
; 0000 00AC 		break;
	RJMP _0xBC
; 0000 00AD         case 4: preamp_menu();
_0xC0:
	CPI  R30,LOW(0x4)
	LDI  R26,HIGH(0x4)
	CPC  R31,R26
	BRNE _0xC1
	RCALL _preamp_menu
; 0000 00AE         break;
	RJMP _0xBC
; 0000 00AF         case 5: loudness_menu();
_0xC1:
	CPI  R30,LOW(0x5)
	LDI  R26,HIGH(0x5)
	CPC  R31,R26
	BRNE _0xC2
	RCALL _loudness_menu
; 0000 00B0         break;
	RJMP _0xBC
; 0000 00B1         case 6: RL_balance_menu();
_0xC2:
	CPI  R30,LOW(0x6)
	LDI  R26,HIGH(0x6)
	CPC  R31,R26
	BRNE _0xC3
	RCALL _RL_balance_menu
; 0000 00B2         break;
	RJMP _0xBC
; 0000 00B3         case 7: FS_balance_menu();
_0xC3:
	CPI  R30,LOW(0x7)
	LDI  R26,HIGH(0x7)
	CPC  R31,R26
	BRNE _0xC4
	RCALL _FS_balance_menu
; 0000 00B4         break;
	RJMP _0xBC
; 0000 00B5         case 8: bluetooth_menu();
_0xC4:
	CPI  R30,LOW(0x8)
	LDI  R26,HIGH(0x8)
	CPC  R31,R26
	BRNE _0xC5
	RCALL _bluetooth_menu
; 0000 00B6         break;
	RJMP _0xBC
; 0000 00B7         case 9: brightless_menu();
_0xC5:
	CPI  R30,LOW(0x9)
	LDI  R26,HIGH(0x9)
	CPC  R31,R26
	BRNE _0xC6
	RCALL _brightless_menu
; 0000 00B8         break;
	RJMP _0xBC
; 0000 00B9         case 10: opasity_menu();
_0xC6:
	CPI  R30,LOW(0xA)
	LDI  R26,HIGH(0xA)
	CPC  R31,R26
	BRNE _0xBC
	RCALL _opasity_menu
; 0000 00BA         break;
; 0000 00BB 	}
_0xBC:
; 0000 00BC };
	RET
;
