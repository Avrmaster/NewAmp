;//Called, when ENCODER has been rotated. 1: clockwise. 2: counterclockwise
;void ValueChanged (char direction) {
; 0000 00C1 void ValueChanged (char direction) {
_ValueChanged:
; 0000 00C2 timer=0;
	ST   -Y,R26
;	direction -> Y+0
	CLR  R6
	CLR  R7
; 0000 00C3 switch (recentMenu) {
	MOV  R30,R5
	LDI  R31,0
	SBRC R30,7
	SER  R31
; 0000 00C4      case 1: if (direction==1) {if (treble<14) {treble++;}} else {if (treble!=0) {treble--;}}
	CPI  R30,LOW(0x1)
	LDI  R26,HIGH(0x1)
	CPC  R31,R26
	BRNE _0xDD
	LD   R26,Y
	CPI  R26,LOW(0x1)
	BRNE _0xDE
	LDI  R30,LOW(14)
	CP   R13,R30
	BRSH _0xDF
	INC  R13
_0xDF:
	RJMP _0xE0
_0xDE:
	TST  R13
	BREQ _0xE1
	DEC  R13
_0xE1:
_0xE0:
; 0000 00C5      break;
	RJMP _0xDC
; 0000 00C6      case 2: if (direction==1) {if (bass<14) {bass++;}} else {if (bass!=0) {bass--;}}
_0xDD:
	CPI  R30,LOW(0x2)
	LDI  R26,HIGH(0x2)
	CPC  R31,R26
	BRNE _0xE2
	LD   R26,Y
	CPI  R26,LOW(0x1)
	BRNE _0xE3
	LDI  R30,LOW(14)
	CP   R12,R30
	BRSH _0xE4
	INC  R12
_0xE4:
	RJMP _0xE5
_0xE3:
	TST  R12
	BREQ _0xE6
	DEC  R12
_0xE6:
_0xE5:
; 0000 00C7      break;
	RJMP _0xDC
; 0000 00C8      case 3: if (direction==1) {if (input<2) {input++;}} else {if (input!=0) {input--;}}
_0xE2:
	CPI  R30,LOW(0x3)
	LDI  R26,HIGH(0x3)
	CPC  R31,R26
	BRNE _0xE7
	LD   R26,Y
	CPI  R26,LOW(0x1)
	BRNE _0xE8
	LDS  R26,_input
	CPI  R26,LOW(0x2)
	BRSH _0xE9
	LDS  R30,_input
	SUBI R30,-LOW(1)
	STS  _input,R30
_0xE9:
	RJMP _0xEA
_0xE8:
	LDS  R30,_input
	CPI  R30,0
	BREQ _0xEB
	SUBI R30,LOW(1)
	STS  _input,R30
_0xEB:
_0xEA:
; 0000 00C9      break;
	RJMP _0xDC
; 0000 00CA      case 4: if (direction==0) {if (gain<24) {gain+=8;}} else {if (gain!=0) {gain-=8;}}
_0xE7:
	CPI  R30,LOW(0x4)
	LDI  R26,HIGH(0x4)
	CPC  R31,R26
	BRNE _0xEC
	LD   R30,Y
	CPI  R30,0
	BRNE _0xED
	LDS  R26,_gain
	CPI  R26,LOW(0x18)
	BRSH _0xEE
	LDS  R30,_gain
	SUBI R30,-LOW(8)
	STS  _gain,R30
_0xEE:
	RJMP _0xEF
_0xED:
	LDS  R30,_gain
	CPI  R30,0
	BREQ _0xF0
	SUBI R30,LOW(8)
	STS  _gain,R30
_0xF0:
_0xEF:
; 0000 00CB      break;
	RJMP _0xDC
; 0000 00CC      case 5: if (direction==0) {if (loudness!=0) {loudness=0;}} else {if (loudness!=4) {loudness=4;}}
_0xEC:
	CPI  R30,LOW(0x5)
	LDI  R26,HIGH(0x5)
	CPC  R31,R26
	BRNE _0xF1
	LD   R30,Y
	CPI  R30,0
	BRNE _0xF2
	LDS  R30,_loudness
	CPI  R30,0
	BREQ _0xF3
	LDI  R30,LOW(0)
	STS  _loudness,R30
_0xF3:
	RJMP _0xF4
_0xF2:
	LDS  R26,_loudness
	CPI  R26,LOW(0x4)
	BREQ _0xF5
	LDI  R30,LOW(4)
	STS  _loudness,R30
_0xF5:
_0xF4:
; 0000 00CD      break;
	RJMP _0xDC
; 0000 00CE      case 6: if (direction==0) {if (LR_balance!=0) {LR_balance--;}} else {if (LR_balance<62) {LR_balance++;}}
_0xF1:
	CPI  R30,LOW(0x6)
	LDI  R26,HIGH(0x6)
	CPC  R31,R26
	BRNE _0xF6
	LD   R30,Y
	CPI  R30,0
	BRNE _0xF7
	LDS  R30,_LR_balance
	CPI  R30,0
	BREQ _0xF8
	SUBI R30,LOW(1)
	STS  _LR_balance,R30
_0xF8:
	RJMP _0xF9
_0xF7:
	LDS  R26,_LR_balance
	CPI  R26,LOW(0x3E)
	BRGE _0xFA
	LDS  R30,_LR_balance
	SUBI R30,-LOW(1)
	STS  _LR_balance,R30
_0xFA:
_0xF9:
; 0000 00CF      break;
	RJMP _0xDC
; 0000 00D0      case 7: if (direction==0) {if (FR_balance!=0) {FR_balance--;}} else {if (FR_balance<62) {FR_balance++;}}
_0xF6:
	CPI  R30,LOW(0x7)
	LDI  R26,HIGH(0x7)
	CPC  R31,R26
	BRNE _0xFB
	LD   R30,Y
	CPI  R30,0
	BRNE _0xFC
	LDS  R30,_FR_balance
	CPI  R30,0
	BREQ _0xFD
	SUBI R30,LOW(1)
	STS  _FR_balance,R30
_0xFD:
	RJMP _0xFE
_0xFC:
	LDS  R26,_FR_balance
	CPI  R26,LOW(0x3E)
	BRGE _0xFF
	LDS  R30,_FR_balance
	SUBI R30,-LOW(1)
	STS  _FR_balance,R30
_0xFF:
_0xFE:
; 0000 00D1      break;
	RJMP _0xDC
; 0000 00D2      case 8: if (direction==0) {if (PINA.1==1) {BluetoothOFF();}} else {BluetoothON();}
_0xFB:
	CPI  R30,LOW(0x8)
	LDI  R26,HIGH(0x8)
	CPC  R31,R26
	BRNE _0x100
	LD   R30,Y
	CPI  R30,0
	BRNE _0x101
	SBIC 0x19,1
	RCALL _BluetoothOFF
	RJMP _0x103
_0x101:
	RCALL _BluetoothON
_0x103:
; 0000 00D3      break;
	RJMP _0xDC
; 0000 00D4      case 9: if (direction==0) {if (brightless>0) {brightless-=5;;}} else {if (brightless<255) {brightless+=5;}}
_0x100:
	CPI  R30,LOW(0x9)
	LDI  R26,HIGH(0x9)
	CPC  R31,R26
	BRNE _0x104
	LD   R30,Y
	CPI  R30,0
	BRNE _0x105
	LDS  R26,_brightless
	CPI  R26,LOW(0x1)
	BRLO _0x106
	LDS  R30,_brightless
	SUBI R30,LOW(5)
	STS  _brightless,R30
_0x106:
	RJMP _0x107
_0x105:
	LDS  R26,_brightless
	CPI  R26,LOW(0xFF)
	BRSH _0x108
	LDS  R30,_brightless
	SUBI R30,-LOW(5)
	STS  _brightless,R30
_0x108:
_0x107:
; 0000 00D5      break;
	RJMP _0xDC
; 0000 00D6      case 10: if (direction==0) {if (opasity>0) {opasity-=10;;}} else {if (opasity<400) {opasity+=10;;}}
_0x104:
	CPI  R30,LOW(0xA)
	LDI  R26,HIGH(0xA)
	CPC  R31,R26
	BRNE _0x10E
	LD   R30,Y
	CPI  R30,0
	BRNE _0x10A
	LDS  R26,_opasity
	LDS  R27,_opasity+1
	CALL __CPW02
	BRSH _0x10B
	CALL SUBOPT_0x16
	SBIW R30,10
	STS  _opasity,R30
	STS  _opasity+1,R31
_0x10B:
	RJMP _0x10C
_0x10A:
	LDS  R26,_opasity
	LDS  R27,_opasity+1
	CPI  R26,LOW(0x190)
	LDI  R30,HIGH(0x190)
	CPC  R27,R30
	BRSH _0x10D
	CALL SUBOPT_0x16
	ADIW R30,10
	STS  _opasity,R30
	STS  _opasity+1,R31
_0x10D:
_0x10C:
; 0000 00D7      break;
	RJMP _0xDC
; 0000 00D8      default: recentMenu=0; if (direction==1) {if (volume<63) {volume++;}} else {if (volume!=0) {volume--;}}
_0x10E:
	CLR  R5
	LD   R26,Y
	CPI  R26,LOW(0x1)
	BRNE _0x10F
	LDI  R30,LOW(63)
	CP   R10,R30
	BRSH _0x110
	INC  R10
_0x110:
	RJMP _0x111
_0x10F:
	TST  R10
	BREQ _0x112
	DEC  R10
_0x112:
_0x111:
; 0000 00D9      break;
; 0000 00DA }
_0xDC:
; 0000 00DB UpdateMenu();
	RCALL _UpdateMenu
; 0000 00DC }
	JMP  _0x2080002
;
;//Ext0 interrupt calling, when at least one BUTTON has been pressed//
;interrupt [EXT_INT0] void Button_Pressed(void) {
; 0000 00DF interrupt [2] void Button_Pressed(void) {
_Button_Pressed:
	CALL SUBOPT_0x17
; 0000 00E0 #asm("sei");
	sei
; 0000 00E1 timer=0;
	CLR  R6
	CLR  R7
; 0000 00E2 delay_ms(10); if (doesTurningOff!=1) {
	CALL SUBOPT_0x1A
	LDS  R26,_doesTurningOff
	CPI  R26,LOW(0x1)
	BREQ _0x113
; 0000 00E3 if (PING&1==1) {button_1();}
	LDS  R30,99
	ANDI R30,LOW(0x1)
	BREQ _0x114
	RCALL _button_1
; 0000 00E4 if ((PING>>1)&1==1) {button_2();}
_0x114:
	LDS  R30,99
	LDI  R31,0
	ASR  R31
	ROR  R30
	ANDI R30,LOW(0x1)
	BREQ _0x115
	RCALL _button_2
; 0000 00E5 if (PINC.0==1) {button_3();}
_0x115:
	SBIC 0x13,0
	RCALL _button_3
; 0000 00E6 if (PINC.1==1) {button_4();}
	SBIC 0x13,1
	RCALL _button_4
; 0000 00E7 if (PINC.2==1) {button_5();}
	SBIC 0x13,2
	RCALL _button_5
; 0000 00E8 if (PINC.3==1) {button_6();}
	SBIC 0x13,3
	RCALL _button_6
; 0000 00E9 if (PINC.4==1) {button_7();}
	SBIC 0x13,4
	RCALL _button_7
; 0000 00EA if (PINC.5==1) {button_8();}
	SBIC 0x13,5
	RCALL _button_8
; 0000 00EB if (PINC.6==1) {button_9();}
	SBIC 0x13,6
	RCALL _button_9
; 0000 00EC }
; 0000 00ED }
_0x113:
	RJMP _0x147
;
