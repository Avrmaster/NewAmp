;//Bluetooth Mute (0 - enable, 1 - disable)
;interrupt [EXT_INT4] void Bluetooth_Mute(void) {
; 0000 00F6 interrupt [6] void Bluetooth_Mute(void) {
_Bluetooth_Mute:
	CALL SUBOPT_0x17
; 0000 00F7 if (input==1) if (PINE.4==1) {
	LDS  R26,_input
	CPI  R26,LOW(0x1)
	BRNE _0x11D
	SBIS 0x1,4
	RJMP _0x11E
; 0000 00F8               i2c_start();
	CALL SUBOPT_0x5
; 0000 00F9 		      i2c_write(0b10001000);
; 0000 00FA               i2c_write(63-volume);
	LDI  R30,LOW(63)
	SUB  R30,R10
	MOV  R26,R30
	RJMP _0x145
; 0000 00FB 		      i2c_stop();
; 0000 00FC               } else {
_0x11E:
; 0000 00FD               i2c_start();
	CALL SUBOPT_0x5
; 0000 00FE 		      i2c_write(0b10001000);
; 0000 00FF               i2c_write(63);
	LDI  R26,LOW(63)
_0x145:
	CALL _i2c_write
; 0000 0100 		      i2c_stop();
	CALL _i2c_stop
; 0000 0101               }
; 0000 0102 
; 0000 0103 }
_0x11D:
	RJMP _0x147
;
;//Ext1 interrupt calling, when IR signal has been detected//
;interrupt [EXT_INT6] void TSOP(void) {
; 0000 0106 interrupt [8] void TSOP(void) {
_TSOP:
; 0000 0107 
; 0000 0108 }
	RETI
;
;
;
;//Called 140 times per second
;interrupt [TIM3_COMPA] void tim3_compa (void) {
; 0000 010D interrupt [27] void tim3_compa (void) {
_tim3_compa:
	CALL SUBOPT_0x17
; 0000 010E     unsigned int enc;
; 0000 010F 	enc=PIND; enc>>=4; enc&=0b00000011;
	ST   -Y,R17
	ST   -Y,R16
;	enc -> R16,R17
	IN   R16,16
	CLR  R17
	MOVW R30,R16
	CALL __LSRW4
	MOVW R16,R30
	__ANDWRN 16,17,3
; 0000 0110 	if (enc!=encwas) {
	LDS  R30,_encwas
	LDS  R31,_encwas+1
	CP   R30,R16
	CPC  R31,R17
	BREQ _0x120
; 0000 0111 		if (encwas==3) {
	LDS  R26,_encwas
	LDS  R27,_encwas+1
	SBIW R26,3
	BRNE _0x121
; 0000 0112 			if (enc==1)  {ValueChanged(0);}
	LDI  R30,LOW(1)
	LDI  R31,HIGH(1)
	CP   R30,R16
	CPC  R31,R17
	BRNE _0x122
	LDI  R26,LOW(0)
	RCALL _ValueChanged
; 0000 0113 			if (enc==2)  {ValueChanged(1);}
_0x122:
	LDI  R30,LOW(2)
	LDI  R31,HIGH(2)
	CP   R30,R16
	CPC  R31,R17
	BRNE _0x123
	LDI  R26,LOW(1)
	RCALL _ValueChanged
; 0000 0114 			UpdateMenu();
_0x123:
	RCALL _UpdateMenu
; 0000 0115 		}
; 0000 0116 		encwas=enc;
_0x121:
	__PUTWMRN _encwas,0,16,17
; 0000 0117     }
; 0000 0118 
; 0000 0119     if (recentMenu!=0) {
_0x120:
	TST  R5
	BREQ _0x124
; 0000 011A       timer++;
	MOVW R30,R6
	ADIW R30,1
	MOVW R6,R30
; 0000 011B       if (timer>700) {
	LDI  R30,LOW(700)
	LDI  R31,HIGH(700)
	CP   R30,R6
	CPC  R31,R7
	BRSH _0x125
; 0000 011C       timer=0;
	CLR  R6
	CLR  R7
; 0000 011D       recentMenu=0;
	CLR  R5
; 0000 011E       UpdateMenu();
	RCALL _UpdateMenu
; 0000 011F       }
; 0000 0120     }
_0x125:
; 0000 0121 
; 0000 0122     if (doesBlueReceiving) {
_0x124:
	TST  R11
	BREQ _0x126
; 0000 0123     BlueTimer++;
	MOVW R30,R8
	ADIW R30,1
	MOVW R8,R30
; 0000 0124     }
; 0000 0125 
; 0000 0126   }
_0x126:
_0x148:
	LD   R16,Y+
	LD   R17,Y+
_0x147:
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R27,Y+
	LD   R26,Y+
	LD   R25,Y+
	LD   R24,Y+
	LD   R23,Y+
	LD   R22,Y+
	LD   R15,Y+
	LD   R1,Y+
	LD   R0,Y+
	RETI
;
;void main(void) {
; 0000 0128 void main(void) {
_main:
; 0000 0129 
; 0000 012A DDRA =0b00011111; DDRB =0b11111111; DDRD =0b00000000; DDRE= 0b00001110; DDRF= 0b00101010; DDRG =0b00001000; DDRC =0b10000000;
	LDI  R30,LOW(31)
	OUT  0x1A,R30
	LDI  R30,LOW(255)
	OUT  0x17,R30
	LDI  R30,LOW(0)
	OUT  0x11,R30
	LDI  R30,LOW(14)
	OUT  0x2,R30
	LDI  R30,LOW(42)
	STS  97,R30
	LDI  R30,LOW(8)
	STS  100,R30
	LDI  R30,LOW(128)
	OUT  0x14,R30
; 0000 012B PORTA=0b00011010; PORTB=0b00010000; PORTD=0b11110010; PORTE=0b01000000; PORTF=0b00001000; PORTG=0b00000011; PORTC=0b11111111;
	LDI  R30,LOW(26)
	OUT  0x1B,R30
	LDI  R30,LOW(16)
	OUT  0x18,R30
	LDI  R30,LOW(242)
	OUT  0x12,R30
	LDI  R30,LOW(64)
	OUT  0x3,R30
	LDI  R30,LOW(8)
	STS  98,R30
	LDI  R30,LOW(3)
	STS  101,R30
	LDI  R30,LOW(255)
	OUT  0x15,R30
; 0000 012C 
; 0000 012D TCCR1A=0b10000010;
	LDI  R30,LOW(130)
	OUT  0x2F,R30
; 0000 012E TCCR1B=0b00000001;
	LDI  R30,LOW(1)
	OUT  0x2E,R30
; 0000 012F ICR1=480;
	LDI  R30,LOW(480)
	LDI  R31,HIGH(480)
	OUT  0x26+1,R31
	OUT  0x26,R30
; 0000 0130 OCR1A=480;
	OUT  0x2A+1,R31
	OUT  0x2A,R30
; 0000 0131 
; 0000 0132 TCCR0=0b01101001;
	LDI  R30,LOW(105)
	OUT  0x33,R30
; 0000 0133 OCR0=0;
	LDI  R30,LOW(0)
	OUT  0x31,R30
; 0000 0134 
; 0000 0135 TCCR3A=0b00000000;
	STS  139,R30
; 0000 0136 TCCR3B=0b00000001;
	LDI  R30,LOW(1)
	STS  138,R30
; 0000 0137 ETIMSK=0b00010000;
	LDI  R30,LOW(16)
	STS  125,R30
; 0000 0138 OCR3AH=0b00001110;
	LDI  R30,LOW(14)
	STS  135,R30
; 0000 0139 OCR3AL=0b10100000;
	LDI  R30,LOW(160)
	STS  134,R30
; 0000 013A 
; 0000 013B #asm("cli");
	cli
; 0000 013C 
; 0000 013D 
; 0000 013E UCSR1A=0x00;
	CALL SUBOPT_0x19
; 0000 013F UCSR1B=0b11011000;       // Uart init:
; 0000 0140 UCSR1C=0b00000110;       // 9600 bouds / 8 data Bit / 1 Stop Bit / No parity
; 0000 0141 UBRR1H=0x00;             // Interrupts RXC and TXC are enabled
; 0000 0142 UBRR1L=0x08;
	LDI  R30,LOW(8)
	STS  153,R30
; 0000 0143 
; 0000 0144 
; 0000 0145 
; 0000 0146 EICRA=0b00001011;           // Interrupts setup:
	LDI  R30,LOW(11)
	STS  106,R30
; 0000 0147 EICRB=0b00100001;           // INT0 - Rising edge interrupt (Buttons); INT1 - falling edge interrupt (ENC BUTTON);
	LDI  R30,LOW(33)
	OUT  0x3A,R30
; 0000 0148 EIMSK=0b01010011;           // INT2 & INT3  - UART (unused ints); INT4 - Any change, BLUETOOTH MUTE; INT6 - Falling edge, TSOP;
	LDI  R30,LOW(83)
	OUT  0x39,R30
; 0000 0149 
; 0000 014A delay_ms(1000);
	CALL SUBOPT_0x1E
; 0000 014B lcd_init(16);
	LDI  R26,LOW(16)
	CALL _lcd_init
; 0000 014C lcd_clear();
	RCALL _lcd_clear
; 0000 014D /*
; 0000 014E while ((PING&1)==0) {
; 0000 014F LED(3); for (temp=0; temp<100; temp++) {if (PING&1==1) break; delay_ms(1);}
; 0000 0150 LED(0); for (temp=0; temp<5000; temp++) {if (PING&1==1) break; delay_ms(1);}
; 0000 0151 }
; 0000 0152 while ((PING&1)==1) {}
; 0000 0153 */
; 0000 0154 
; 0000 0155 LED(2);
	LDI  R26,LOW(2)
	RCALL _LED
; 0000 0156 PORTA|=1;
	SBI  0x1B,0
; 0000 0157 
; 0000 0158 delay_ms(100);
	LDI  R26,LOW(100)
	CALL SUBOPT_0x1D
; 0000 0159 
; 0000 015A 
; 0000 015B //Writing New Symbols into LCD
; 0000 015C 
; 0000 015D {
; 0000 015E define_char(char0,0);
	LDI  R30,LOW(_char0*2)
	LDI  R31,HIGH(_char0*2)
	ST   -Y,R31
	ST   -Y,R30
	LDI  R26,LOW(0)
	CALL _define_char
; 0000 015F define_char(char1,1);
	LDI  R30,LOW(_char1*2)
	LDI  R31,HIGH(_char1*2)
	ST   -Y,R31
	ST   -Y,R30
	LDI  R26,LOW(1)
	CALL _define_char
; 0000 0160 define_char(char2,2);
	LDI  R30,LOW(_char2*2)
	LDI  R31,HIGH(_char2*2)
	ST   -Y,R31
	ST   -Y,R30
	LDI  R26,LOW(2)
	CALL _define_char
; 0000 0161 define_char(char3,3);
	LDI  R30,LOW(_char3*2)
	LDI  R31,HIGH(_char3*2)
	ST   -Y,R31
	ST   -Y,R30
	LDI  R26,LOW(3)
	CALL _define_char
; 0000 0162 }
; 0000 0163 
; 0000 0164 
; 0000 0165 
; 0000 0166 i2c_init();//Test
	CALL _i2c_init
; 0000 0167 i2c_start();
	CALL _i2c_start
; 0000 0168 i2c_stop();
	CALL _i2c_stop
; 0000 0169 
; 0000 016A 
; 0000 016B if (i2c_start()==1) {
	CALL _i2c_start
	CPI  R30,LOW(0x1)
	BRNE _0x127
; 0000 016C     if (i2c_write(0b10001000)==1) {}
	LDI  R26,LOW(136)
	CALL _i2c_write
	CPI  R30,LOW(0x1)
	BREQ _0x129
; 0000 016D     else {
; 0000 016E     lcd_clear();
	RCALL _lcd_clear
; 0000 016F     lcd_puts("Audio processor");
	__POINTW2MN _0x12A,0
	RCALL _lcd_puts
; 0000 0170     lcd_gotoxy(0,2);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R26,LOW(2)
	RCALL _lcd_gotoxy
; 0000 0171     lcd_puts("Error: adress");
	__POINTW2MN _0x12A,16
	RCALL _lcd_puts
; 0000 0172     i2c_stop();
	CALL _i2c_stop
; 0000 0173     while (1) {PORTF=0b00000100; delay_ms(1000); PORTF=0x00; delay_ms(1000);}}
_0x12B:
	LDI  R30,LOW(4)
	CALL SUBOPT_0x1F
	LDI  R30,LOW(0)
	CALL SUBOPT_0x1F
	RJMP _0x12B
_0x129:
; 0000 0174     }
; 0000 0175 
; 0000 0176 else {
	RJMP _0x12E
_0x127:
; 0000 0177 lcd_clear();
	RCALL _lcd_clear
; 0000 0178 lcd_puts("Audio processor");
	__POINTW2MN _0x12A,30
	CALL SUBOPT_0x1
; 0000 0179 lcd_gotoxy(0,1);
; 0000 017A lcd_puts("Error with bus");
	__POINTW2MN _0x12A,46
	RCALL _lcd_puts
; 0000 017B i2c_stop();
	CALL _i2c_stop
; 0000 017C while (1) {PORTF=0b00000100; delay_ms(1000); PORTF=0x00; delay_ms(1000);}
_0x12F:
	LDI  R30,LOW(4)
	CALL SUBOPT_0x1F
	LDI  R30,LOW(0)
	CALL SUBOPT_0x1F
	RJMP _0x12F
; 0000 017D }
_0x12E:
; 0000 017E 
; 0000 017F 
; 0000 0180 #asm ("sei");
	sei
; 0000 0181 
; 0000 0182 
; 0000 0183 //Writing default settings;
; 0000 0184 
; 0000 0185     if (doesEEpromWritten!=1) {
	LDI  R26,LOW(_doesEEpromWritten)
	LDI  R27,HIGH(_doesEEpromWritten)
	CALL __EEPROMRDB
	CPI  R30,LOW(0x1)
	BREQ _0x132
; 0000 0186     volume=10;
	LDI  R30,LOW(10)
	MOV  R10,R30
; 0000 0187 	bass=10;
	MOV  R12,R30
; 0000 0188 	treble=13;
	LDI  R30,LOW(13)
	MOV  R13,R30
; 0000 0189 	gain=0b00011000;
	LDI  R30,LOW(24)
	STS  _gain,R30
; 0000 018A 	loudness=0;
	LDI  R30,LOW(0)
	STS  _loudness,R30
; 0000 018B     LR_balance=31;
	LDI  R30,LOW(31)
	STS  _LR_balance,R30
; 0000 018C     FR_balance=31;
	STS  _FR_balance,R30
; 0000 018D     input=1;
	LDI  R30,LOW(1)
	STS  _input,R30
; 0000 018E     brightless=125;
	LDI  R30,LOW(125)
	STS  _brightless,R30
; 0000 018F     opasity=220;
	LDI  R30,LOW(220)
	LDI  R31,HIGH(220)
	RJMP _0x146
; 0000 0190     } else {
_0x132:
; 0000 0191     volume=settings[0];
	LDI  R26,LOW(_settings)
	LDI  R27,HIGH(_settings)
	CALL __EEPROMRDB
	MOV  R10,R30
; 0000 0192 	bass=settings[1];
	__POINTW2MN _settings,2
	CALL __EEPROMRDB
	MOV  R12,R30
; 0000 0193 	treble=settings[2];
	__POINTW2MN _settings,4
	CALL __EEPROMRDB
	MOV  R13,R30
; 0000 0194 	gain=settings[3];
	__POINTW2MN _settings,6
	CALL __EEPROMRDB
	STS  _gain,R30
; 0000 0195 	loudness=settings[4];
	__POINTW2MN _settings,8
	CALL __EEPROMRDB
	STS  _loudness,R30
; 0000 0196     LR_balance=settings[5];
	__POINTW2MN _settings,10
	CALL __EEPROMRDB
	STS  _LR_balance,R30
; 0000 0197     FR_balance=settings[6];
	__POINTW2MN _settings,12
	CALL __EEPROMRDB
	STS  _FR_balance,R30
; 0000 0198     input=settings[7];
	__POINTW2MN _settings,14
	CALL __EEPROMRDB
	STS  _input,R30
; 0000 0199     brightless=settings[8];
	__POINTW2MN _settings,16
	CALL __EEPROMRDB
	STS  _brightless,R30
; 0000 019A     opasity=settings[9];
	__POINTW2MN _settings,18
	CALL __EEPROMRDW
_0x146:
	STS  _opasity,R30
	STS  _opasity+1,R31
; 0000 019B     }
; 0000 019C 
; 0000 019D 
; 0000 019E for (recentMenu=0; recentMenu<11; recentMenu++) {
	CLR  R5
_0x135:
	LDI  R30,LOW(11)
	CP   R5,R30
	BRGE _0x136
; 0000 019F UpdateMenu();
	RCALL _UpdateMenu
; 0000 01A0 }
	INC  R5
	RJMP _0x135
_0x136:
; 0000 01A1 recentMenu=0;
	CLR  R5
; 0000 01A2 UpdateMenu();
	RCALL _UpdateMenu
; 0000 01A3 
; 0000 01A4 BluetoothOFF();
	RCALL _BluetoothOFF
; 0000 01A5 delay_ms(10000);
	LDI  R26,LOW(10000)
	LDI  R27,HIGH(10000)
	CALL _delay_ms
; 0000 01A6 BluetoothON();
	RCALL _BluetoothON
; 0000 01A7 delay_ms(2000);
	LDI  R26,LOW(2000)
	LDI  R27,HIGH(2000)
	CALL _delay_ms
; 0000 01A8 SendToBluetooth("CB");
	__POINTW2MN _0x12A,61
	RCALL _SendToBluetooth
; 0000 01A9 SendToBluetooth("CC");
	__POINTW2MN _0x12A,64
	RCALL _SendToBluetooth
; 0000 01AA 
; 0000 01AB while (1) {
_0x137:
; 0000 01AC 
; 0000 01AD }
	RJMP _0x137
; 0000 01AE 
; 0000 01AF 
; 0000 01B0 }
_0x13A:
	RJMP _0x13A

	.DSEG
_0x12A:
	.BYTE 0x43
	#ifndef __SLEEP_DEFINED__
	#define __SLEEP_DEFINED__
	.EQU __se_bit=0x20
	.EQU __sm_mask=0x1C
	.EQU __sm_powerdown=0x10
	.EQU __sm_powersave=0x18
	.EQU __sm_standby=0x14
	.EQU __sm_ext_standby=0x1C
	.EQU __sm_adc_noise_red=0x08
	.SET power_ctrl_reg=mcucr
	#endif

	.DSEG

	.CSEG
__lcd_write_nibble_G100:
	ST   -Y,R26
	LD   R30,Y
	ANDI R30,LOW(0x10)
	BREQ _0x2000004
	SBI  0x3,2
	RJMP _0x2000005
_0x2000004:
	CBI  0x3,2
_0x2000005:
	LD   R30,Y
	ANDI R30,LOW(0x20)
	BREQ _0x2000006
	SBI  0x3,1
	RJMP _0x2000007
_0x2000006:
	CBI  0x3,1
_0x2000007:
	LD   R30,Y
	ANDI R30,LOW(0x40)
	BREQ _0x2000008
	SBI  0x18,7
	RJMP _0x2000009
_0x2000008:
	CBI  0x18,7
_0x2000009:
	LD   R30,Y
	ANDI R30,LOW(0x80)
	BREQ _0x200000A
	LDS  R30,101
	ORI  R30,8
	RJMP _0x200001D
_0x200000A:
	LDS  R30,101
	ANDI R30,0XF7
_0x200001D:
	STS  101,R30
	__DELAY_USB 11
	SBI  0x18,2
	__DELAY_USB 27
	CBI  0x18,2
	__DELAY_USB 27
	RJMP _0x2080002
__lcd_write_data:
	ST   -Y,R26
	LD   R26,Y
	RCALL __lcd_write_nibble_G100
    ld    r30,y
    swap  r30
    st    y,r30
	LD   R26,Y
	RCALL __lcd_write_nibble_G100
	__DELAY_USW 200
	RJMP _0x2080002
_lcd_write_byte:
	ST   -Y,R26
	LDD  R26,Y+1
	RCALL __lcd_write_data
	SBI  0x18,0
	LD   R26,Y
	RCALL __lcd_write_data
	CBI  0x18,0
	RJMP _0x2080003
_lcd_gotoxy:
	ST   -Y,R26
	LD   R30,Y
	LDI  R31,0
	SUBI R30,LOW(-__base_y_G100)
	SBCI R31,HIGH(-__base_y_G100)
	LD   R30,Z
	LDD  R26,Y+1
	ADD  R26,R30
	RCALL __lcd_write_data
	LDD  R30,Y+1
	STS  __lcd_x,R30
	LD   R30,Y
	STS  __lcd_y,R30
_0x2080003:
	ADIW R28,2
	RET
_lcd_clear:
	LDI  R26,LOW(2)
	RCALL __lcd_write_data
	LDI  R26,LOW(3)
	CALL SUBOPT_0x1D
	LDI  R26,LOW(12)
	RCALL __lcd_write_data
	LDI  R26,LOW(1)
	RCALL __lcd_write_data
	LDI  R26,LOW(3)
	CALL SUBOPT_0x1D
	LDI  R30,LOW(0)
	STS  __lcd_y,R30
	STS  __lcd_x,R30
	RET
_lcd_putchar:
	ST   -Y,R26
	LD   R26,Y
	CPI  R26,LOW(0xA)
	BREQ _0x2000011
	LDS  R30,__lcd_maxx
	LDS  R26,__lcd_x
	CP   R26,R30
	BRLO _0x2000010
_0x2000011:
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDS  R26,__lcd_y
	SUBI R26,-LOW(1)
	STS  __lcd_y,R26
	RCALL _lcd_gotoxy
	LD   R26,Y
	CPI  R26,LOW(0xA)
	BRNE _0x2000013
	RJMP _0x2080002
_0x2000013:
_0x2000010:
	LDS  R30,__lcd_x
	SUBI R30,-LOW(1)
	STS  __lcd_x,R30
	SBI  0x18,0
	LD   R26,Y
	RCALL __lcd_write_data
	CBI  0x18,0
	RJMP _0x2080002
_lcd_puts:
	ST   -Y,R27
	ST   -Y,R26
	ST   -Y,R17
_0x2000014:
	LDD  R26,Y+1
	LDD  R27,Y+1+1
	LD   R30,X+
	STD  Y+1,R26
	STD  Y+1+1,R27
	MOV  R17,R30
	CPI  R30,0
	BREQ _0x2000016
	MOV  R26,R17
	RCALL _lcd_putchar
	RJMP _0x2000014
_0x2000016:
	LDD  R17,Y+0
	ADIW R28,3
	RET
_lcd_init:
	ST   -Y,R26
	SBI  0x2,2
	SBI  0x2,1
	SBI  0x17,7
	LDS  R30,100
	ORI  R30,8
	STS  100,R30
	SBI  0x17,2
	SBI  0x17,0
	SBI  0x2,3
	CBI  0x18,2
	CBI  0x18,0
	CBI  0x3,3
	LD   R30,Y
	STS  __lcd_maxx,R30
	SUBI R30,-LOW(128)
	__PUTB1MN __base_y_G100,2
	LD   R30,Y
	SUBI R30,-LOW(192)
